{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure/javascript/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "disable_contribution_on_translation_quality": ["MT"],
      "feedback_system": "None",
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "MSDN.node-api",
          "folder_relative_path_in_docset": "docs-ref-conceptual"
        }
      }
    },
    "fileMetadata": {},
    "template": [],
    "dest": "azure-javascript",
    "markdownEngineName": "markdig"
  }
}
